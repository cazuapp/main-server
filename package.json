{
  "name": "cazuapp-server",
  "version": "1.0.0",
  "description": "API Server for CazuApp",
  "url": "https://www.cazuapp.dev",
  "scripts": {
    "start": "cross-env DEBUG=express-sequelize node ./server.js",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js"
  },
  "nodemonConfig": {
    "watch": [
      "handlers/",
      "html/",
      "server.js",
      "routes/"
    ],
    "ext": "js,json,html,ejs"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "3.7.2",
    "bull": "4.12.2",
    "cli-progress": "^3.12.0",
    "colors": "^1.4.0",
    "connect-timeout": "1.9.0",
    "console-table-printer": "^2.12.0",
    "debug": "4.3.4",
    "dotenv": "^16.4.2",
    "dotenv-parse-variables": "2.0.0",
    "ejs": "3.1.9",
    "email-validator": "2.0.4",
    "express": "4.18.2",
    "express-list-endpoints": "^6.0.0",
    "express-useragent": "1.0.15",
    "geoip-lite": "1.4.9",
    "getcountry-metadata": "1.0.3",
    "i18n": "0.15.1",
    "is-valid-coordinates": "1.0.0",
    "js-base64": "3.7.6",
    "jsonwebtoken": "9.0.2",
    "kleur": "^4.1.5",
    "md5": "2.3.0",
    "microtime": "3.1.1",
    "moment": "2.30.1",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "mysql2": "3.9.1",
    "nodemailer": "6.9.9",
    "promise": "8.3.0",
    "redis": "^4.6.13",
    "sequelize": "6.37.0",
    "systeminformation": "5.21.24",
    "uuid": "9.0.1",
    "winston": "3.11.0",
    "winston-daily-rotate-file": "5.0.0",
    "winston-transport": "4.7.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "mocha": "^10.3.0"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cazuapp/main-server"
  },
  "homepage": "https://www.cazuapp.dev"
}
